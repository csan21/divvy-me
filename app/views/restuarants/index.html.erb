<div class="restuarants-content">
  <ul>
    <% @restuarants.each do |restuarant| %>
      <li>
        <%= restuarant.name %>
        <%= restuarant.votes.count %>
          <% if restuarant.creator_id != current_user.id %>
            <%= button_to 'vote!', votes_path(id: restuarant.id, method: :post) %>
          <% end %>
      </li>
    <% end %>
  </ul>
</div>
