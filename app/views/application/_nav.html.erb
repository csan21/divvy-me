<div class="container">
  <header>
    <nav>
      <ul class="nav-links">
        <% if logged_in? %>
          <li>
            <button type="button" class="btn btn-primary">
              Login
            </button>
          </li>
        <% else %>
          <li><%= link_to "Log out", logout_path, method: :delete %></li>
        <% end %>
      </ul>
    </nav>
  </header>
</div>
